class A {
	
	void main()
	{
		int a = 1, b = 4;
		int arr[2];
		callout("print_int", a, b);
		b = 4;
		b += a;
		callout("print_str", "+");
		callout("print_int", a, b);
		b = 4;
		b -= a;
		callout("print_str", "-");
		callout("print_int", a, b);
		b = 4;
		b *= a;
		callout("print_str", "*");
		callout("print_int", a, b);
		b = 4;
		b /= a;
		callout("print_str", "/");
		callout("print_int", a, b);
		b = 4;
		b %= a;
		callout("print_str", "%");
		callout("print_int", a, b);
		b = 4;
		b &= a;
		callout("print_str", "&");
		callout("print_int", a, b);
		b = 4;
		b |= a;
		callout("print_str", "|");
		callout("print_int", a, b);
		b = 4;
		b <<= a;
		callout("print_str", "<<");
		callout("print_int", a, b);
		b = 4;
		b >>= a;
		callout("print_str", ">>");
		callout("print_int", a, b);
		arr[0] = 14;
		arr[1] = 15;
		arr[0] += arr[1] + arr[0] - a;
		callout("print_str", "arr");
		callout("print_int", arr[0] , arr[1]);
	}
}
